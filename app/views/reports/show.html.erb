      $(document).ready(function() {
          var r = Raphael("dbausage"),
          txtattr = { font: "12px sans-serif" };

          var allowance = 800; // dba allowance for the term
          var usage = new Array();
          var optimal = new Array();
          var x = new Array();
          for (var i = 0; i < 5*7; i++) {
          usage.push(Math.random() * (allowance / (10*7)));
